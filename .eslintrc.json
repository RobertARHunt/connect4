{
  "env": {
    "browser": true,
    "es2021": true
  },

  "extends": ["plugin:react/recommended", "standard"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "no-console": "error",
    "comma-dangle": "off",
    "semi": "off",
    "react/prop-types": "off",
    "space-before-function-paren": "off",
    "react/react-in-jsx-scope": "off",
    "curly": "off",
    "prefer-const": "error"
  }
}
